{
  "name": "colmena-core-api",
  "version": "1.0.0",
  "description": "",
  "main": "server.ts",
  "engines": {
    "node": "14.x",
    "npm": "6.x"
  },
  "scripts": {
    "test": "jest",
    "dev": "nodemon --ignore '**/logs/.*' --watch 'src/**/*' -e ts,js,json --exec 'ts-node --files src/server.ts'",
    "dev-inspect": "nodemon --ignore '**/logs/.*' --watch 'src/**/*' -e ts,js,json --exec 'npm run inspect'",
    "inspect": "kill-port --port 9229 && TS_NODE_FILES=true node --inspect=0.0.0.0 -r ts-node/register src/server.ts",
    "start": "node ./dist/server.js",
    "profile": "npx 0x -D profiling ./dist/server.js",
    "eslint": "eslint --debug --ext .d.ts,.js,.jsx,.ts,.tsx src",
    "seed": "for filename in ./seeds/*; do mongoimport --mode=upsert --uri ${MONGO_DSN} --file $filename;  done",
    "export-collection": "dump() { mongoexport --uri ${MONGO_DSN} --collection $1 -o \"./seeds/$1.json\"  ; }; dump",
    "export-db": "mongodump --uri ${MONGO_DSN} -o './backups/'$( date -Iseconds)",
    "import-db": "mongorestore --uri ${MONGO_DSN}",
    "export-schema": "mongoexport --uri ${MONGO_DSN} --collection _SCHEMA -o \"./seeds/_SCHEMA.json\"",
    "madge-circular": "madge --circular --extensions ts ./src",
    "madge-orphans": "madge --orphans ./src/cloud/main.js",
    "build": "npm run madge-circular && tsc",
    "tsc": "tsc",
    "prepublish": "npm run build"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@googlemaps/google-maps-services-js": "^2.3.1",
    "@sendgrid/mail": "^6.5.3",
    "axios": "0.19.2",
    "eosjs": "^22.1.0",
    "eosjs-ecc": "^4.0.7",
    "escape-html": "^1.0.3",
    "express": "^4.17.1",
    "hat": "0.0.3",
    "parse": "^3.2.0",
    "parse-dashboard": "^2.1.0",
    "parse-server": "^4.10.19",
    "saslprep": "^1.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-typescript": "^7.9.0",
    "@types/express": "^4.17.6",
    "@types/jest": "^25.2.1",
    "@types/node": "^13.11.1",
    "@types/parse": "^2.10.18",
    "@typescript-eslint/eslint-plugin": "^2.28.0",
    "babel-jest": "^25.3.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-airbnb-typescript": "^7.2.1",
    "eslint-plugin-import": "^2.20.2",
    "jest": "^25.3.0",
    "kill-port": "^1.6.0",
    "madge": "^3.8.0",
    "nodemon": "^2.0.3",
    "prettier": "^1.19.1",
    "ts-node": "^8.8.2",
    "typescript": "^3.8.3"
  }
}
